{
  "name": "archive-view",
  "version": "0.66.0",
  "description": "View the files and folders inside archive files",
  "main": "lib/main.js",
  "dependencies": {
    "etch": "0.9.0",
    "fs-plus": "^3.0.0",
    "humanize-plus": "~1.8.2",
    "ls-archive": "1.3.4",
    "temp": "~0.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^23.0.0",
    "rollup": "^3.2.3",
    "standard": "^11.0.1"
  },
  "files": [
    "dist",
    "keymaps",
    "styles"
